{
  "name": "nessworthy/textmarketer-php-sdk",
  "description": "An unofficial helper to allow you to send SMS messages using Text Marketer.",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Sean Nessworthy",
      "email": "sean@nessworthy.me"
    }
  ],
  "require": {
    "php": ">=7.0",
    "roave/security-advisories": "dev-master",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Nessworthy\\TextMarketer\\" : "src/Nessworthy/TextMarketer/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nessworthy\\TextMarketer\\Tests\\" : "tests/Nessworthy/TextMarketer/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  }
}
