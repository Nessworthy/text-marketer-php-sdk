{
  "name": "nessworthy/textmarketer",
  "description": "An unofficial helper to allow you to send SMS messages using Text Marketer.",
  "minimum-stability": "stable",
  "version": "1.1.4",
  "license": "wtfpl",
  "authors": [
    {
      "name": "Sean Nessworthy",
      "email": "sean@nessworthy.me"
    }
  ],
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "^7.0",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "Nessworthy\\TextMarketer\\" : "src/Nessworthy/TextMarketer/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nessworthy\\TextMarketer\\Test\\Unit\\" : "tests/unit",
      "Nessworthy\\TextMarketer\\Test\\Integration\\" : "tests/integration"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0"
  }
}
